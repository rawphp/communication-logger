{
  "name": "rawphp/communication-logger",
  "description": "Communication logger for PHP applications.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Kaczocha",
      "email": "tom@crazydev.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ramsey/uuid": "^3.4",
    "monolog/monolog": "^1.20"
  },
  "require-dev": {
    "phpspec/phpspec": "3.0.0-beta2",
    "sebastian/phpcpd": "^2.0",
    "squizlabs/php_codesniffer": "^2.6",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "RawPHP\\CommunicationLogger\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}
